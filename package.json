{
  "name": "ts-egg",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-ts-egg",
    "stop": "egg-scripts stop --title=egg-server-ts-egg",
    "dev": "egg-bin dev -r egg-ts-helper/register",
    "debug": "egg-bin debug -r egg-ts-helper/register --workers=1",
    "test-local": "egg-bin test -r egg-ts-helper/register",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov -r egg-ts-helper/register",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean",
    "migrate": "ts-node node_modules/.bin/sequelize"
  },
  "dependencies": {
    "@strange-fish/egg-sequelize-typescript": "^1.0.3",
    "@types/passport-http-bearer": "^1.0.33",
    "bcrypt": "^3.0.6",
    "egg": "^2.22.2",
    "egg-passport": "^2.0.1",
    "egg-redis": "^2.3.2",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^4.3.1",
    "egg-validate": "^2.0.2",
    "mysql2": "^1.6.5",
    "mz-modules": "^2.1.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "patch-package": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^4.44.0",
    "sequelize-cli": "^5.4.0",
    "sequelize-typescript": "^0.6.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.10.6",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.7",
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.13.0",
    "egg-ci": "^1.12.0",
    "egg-mock": "^3.23.1",
    "egg-ts-helper": "^1.25.3",
    "tslib": "^1.9.0",
    "tslint": "^4.0.0",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
